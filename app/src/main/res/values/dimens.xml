    <declare-styleable name="MoneyTextView">
        <attr name="format" format="string"/>
        <attr name="symbol" format="string"/>
        <attr name="amount" format="float"/>
        <attr name="symbolMargin" format="dimension"/>
        <attr name="decimalMargin" format="dimension"/>
        <attr name="decimalSeparator" format="string"/>

        <attr name="baseTextSize" format="dimension"/>
        <attr name="symbolTextSize" format="dimension"/>
        <attr name="decimalDigitsTextSize" format="dimension"/>

        <attr name="symbolGravity">
            <flag name="start" value="0x01"/>
            <flag name="end" value="0x02"/>
            <flag name="top" value="0x04"/>
            <flag name="bottom" value="0x08"/>
        </attr>

        <attr name="decimalGravity" format="enum">
            <enum name="top" value="4"/>
            <enum name="bottom" value="8"/>
        </attr>

        <attr name="gravity">
            <enum name="left" value="0x01"/>
            <enum name="right" value="0x02"/>
            <enum name="top" value="0x04"/>
            <enum name="bottom" value="0x08"/>
            <enum name="centerVertical" value="0x16"/>
            <enum name="centerHorizontal" value="0x32"/>
            <enum name="center" value="0x48"/>
        </attr>

        <attr name="baseTextColor" format="color"/>
        <attr name="symbolTextColor" format="color"/>
        <attr name="decimalTextColor" format="color"/>

        <attr name="includeDecimalSeparator" format="boolean"/>
        <attr name="decimalUnderline" format="boolean"/>
        <attr name="fontPath" format="string"/>
    </declare-styleable>

    <declare-styleable name="TagView">
        <attr name="tagType" format="enum">
            <enum name="classic" value="0"/>
            <enum name="modern" value="1"/>
            <enum name="sharp" value="2"/>
            <enum name="modern_sharp" value="3"/>
            <enum name="reversed_modern" value="4"/>
            <enum name="reversed_sharp" value="5"/>
            <enum name="reversed_modern_sharp" value="6"/>
        </attr>
        <attr name="tagColor" format="color"/>
        <attr name="tagUpperCase" format="boolean"/>
        <attr name="tagBorderRadius" format="integer"/>
        <attr name="tagCircleRadius" format="integer"/>
        <attr name="tagCircleColor" format="color"/>
        <attr name="tagTextColor" format="color"/>
    </declare-styleable>

